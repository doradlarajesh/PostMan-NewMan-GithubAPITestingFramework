		"name": "GitHub APIs",
		"description": "Play and Test Github APIs with Postman",
		"schema": "https://schema.getpostman.com/json/collection/v2.0.0/collection.json"
	},
	"item": [
		{
			"name": "Repositories",
			"item": [
				{
					"name": "Get User Repos",
					"id": "b3449bad-be7d-43c6-9475-8b3bff1ea7eb",
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": {
								"token": "{{usertoken}}"
							}
						},
						"method": "GET",
						"header": [
							{
								"key": "Accept",
								"value": "application/vnd.github+json"
							},
							{
								"key": "Authorization",
								"value": "",
								"disabled": true
							},
							{
								"key": "X-GitHub-Api-Version",
								"value": "2022-11-28"
							}
						],
						"url": {
							"raw": "https://api.github.com/users/:USERNAME/repos",
							"protocol": "https",
							"host": [
								"api",
								"github",
								"com"
							],
							"path": [
								"users",
								":USERNAME",
								"repos"
							],
							"variable": [
								{
									"key": "USERNAME",
									"value": "{{GitUserName}}"
								}
							]
						}
					},
					"response": []
				}
			],
			"id": "0de3309b-33dd-4499-bb9b-65155b383f59"
		}
	],
	"auth": {
		"type": "bearer",
		"bearer": {
			"token": "Bearer {{usertoken}}"
		}
	},
